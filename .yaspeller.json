{
  "excludeFiles": [
    ".git",
    ".idea",
    ".vscode",
    "node_modules",
    "build",
    "coverage",
    ".nyc_output"
  ],
  "lang": "en",
  "fileExtensions": [".md"],
  "dictionary": [
    "async",
    "asynchronously",
    "AsyncStorage",
    "debounce",
    "fp",
    "IndexedDB",
    "localStorage",
    "patronum",
    "Payload",
    "sessionStorage",
    "StorageAdapter",
    "unhandled",
    "unsubscribe",
    "keyArea",
    "timestamp"
  ],
  "ignoreText": [],
  "ignoreUrls": true,
  "ignoreUppercase": true,
  "findRepeatWords": false,
  "maxRequests": 5
}
