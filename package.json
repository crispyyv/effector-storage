{
  "name": "effector-storage",
  "description": "Module for Effector to sync stores with different storages",
  "version": "4.0.0-rc",
  "author": "Victor Didenko <yumaa.verdin@gmail.com> (https://yumaa.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumauri/effector-storage"
  },
  "bugs": {
    "url": "https://github.com/yumauri/effector-storage/issues"
  },
  "homepage": "https://github.com/yumauri/effector-storage#readme",
  "keywords": [
    "effector",
    "persist",
    "storage",
    "localstorage",
    "local storage",
    "sessionstorage",
    "session storage"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup --config",
    "build:dev": "rollup --config",
    "check:lint": "eslint src --ext .ts",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check src",
    "check:spell": "yaspeller .",
    "lint": "npm-run-all --parallel check:*",
    "format": "prettier --write src tests",
    "test": "npm-run-all --parallel test:*",
    "test:node": "nyc --reporter=lcov --reporter=text uvu -r ts-node/register -i mock -i types tests",
    "test:types": "uvu -r ts-node/register tests types",
    "clean": "rm -rf build coverage .nyc_output",
    "size": "size-limit --highlight-less",
    "release": "np --yolo --no-release-draft --no-2fa --contents ./build --tag next",
    "version": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-block-scoping": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@size-limit/preset-small-lib": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "effector": "21.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "flowgen": "^1.12.0",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patronum": "^0.111.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^4.7.0",
    "snoop": "^1.0.2",
    "ts-node": "^9.0.0",
    "tsd": "^0.13.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "uvu": "^0.3.4",
    "yaspeller": "^7.0.0"
  }
}
