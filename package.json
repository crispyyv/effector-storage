{
  "name": "effector-storage",
  "description": "Module for Effector to sync stores with different storages",
  "version": "4.0.0-beta",
  "author": "Victor Didenko <yumaa.verdin@gmail.com> (https://yumaa.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumauri/effector-storage"
  },
  "bugs": {
    "url": "https://github.com/yumauri/effector-storage/issues"
  },
  "homepage": "https://github.com/yumauri/effector-storage#readme",
  "keywords": [
    "effector",
    "persist",
    "localstorage",
    "local storage",
    "sessionstorage",
    "session storage"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup --config",
    "build:dev": "rollup --config",
    "check:lint": "eslint src --ext .ts",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check src",
    "lint": "npm-run-all --parallel check:*",
    "format": "prettier --write src tests",
    "test:node": "nyc --reporter=lcov --reporter=text uvu -r ts-node/register -i mock tests",
    "test:types": "uvu -r ts-node/register tests types",
    "clean": "rm -rf build coverage .nyc_output",
    "size": "size-limit",
    "release": "np --yolo --no-release-draft --no-2fa --contents ./build --tag next",
    "version": "yarn build"
  },
  "dependencies": {
    "effector": ">20"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "esbuild": "^0.7.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "flowgen": "^1.11.0",
    "np": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rollup": "^2.27.0",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-esbuild": "^2.5.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "size-limit": "^4.6.0",
    "snoop": "^1.0.2",
    "ts-node": "^9.0.0",
    "tsd": "^0.13.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "uvu": "^0.3.3"
  }
}
