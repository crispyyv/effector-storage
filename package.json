{
  "name": "effector-storage",
  "description": "Module for Effector to sync stores with different storages",
  "version": "4.3.0",
  "author": "Victor Didenko <yumaa.verdin@gmail.com> (https://yumaa.name)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumauri/effector-storage"
  },
  "bugs": {
    "url": "https://github.com/yumauri/effector-storage/issues"
  },
  "homepage": "https://github.com/yumauri/effector-storage#readme",
  "keywords": [
    "effector",
    "persist",
    "storage",
    "localstorage",
    "local storage",
    "sessionstorage",
    "session storage"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup --config",
    "build:dev": "rollup --config",
    "check:lint": "eslint src --ext .ts",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check src",
    "check:spell": "yaspeller .",
    "lint": "npm-run-all --parallel check:*",
    "format": "prettier --write src tests",
    "test": "npm-run-all --parallel test:*",
    "test:node": "nyc --reporter=lcov --reporter=text uvu -r ts-node/register -i mock -i types tests",
    "test:types": "uvu -r ts-node/register tests types",
    "doc:update-readme": "npx doctoc --maxlevel 3 README.md",
    "clean": "rm -rf build coverage .nyc_output",
    "size": "size-limit --highlight-less",
    "release": "np --yolo --no-release-draft --no-2fa --contents ./build",
    "version": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-block-scoping": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "effector": "21.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "flowgen": "^1.14.1",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patronum": "^1.1.0",
    "prettier": "^2.3.2",
    "rollup": "^2.53.2",
    "rollup-plugin-command": "^1.1.3",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^5.0.1",
    "snoop": "^1.0.2",
    "ts-node": "^10.1.0",
    "tsd": "^0.17.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "uvu": "^0.5.1",
    "yaspeller": "^7.0.0"
  }
}
